<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Oskari</display-name>

     <servlet>
        <servlet-name>mapFullServlet</servlet-name>
        <servlet-class>fi.nls.oskari.map.servlet.MapFullServlet</servlet-class>
        <init-param>
            <param-name>version</param-name>
            <param-value>Use property oskari.client.version to configure used client version or type it here</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>mapFullServlet</servlet-name>
         <url-pattern>/main</url-pattern>
        <url-pattern>/service</url-pattern>
    </servlet-mapping>

	<welcome-file-list>
		<welcome-file>main</welcome-file>
	</welcome-file-list>
</web-app>